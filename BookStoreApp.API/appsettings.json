{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "WriteTo": [
    {
      "Name": "Console",
      "Args": {
        "path": "./logs/log-.txt",
        "rollingInterval": "Day"
      }
    }
  ],
  "AllowedHosts": "*"
}
